tpfa_2.MouseButton1Down:connect(function()
	if not tpfa_2.Text == "Insta Re" then return end
	tpfa_2.Text = "Instant respawn (ENABLED)"
	--if not _G.instarespawn then
	local player = game.Players.LocalPlayer
	local character = player.Character
	--character:WaitForChild("Humanoid").Died:Connect(function()
	while wait(1.5) do
		if player.PlayerGui.Death.respawn.Visible == true and character.Humanoid.Health == 0 and not character:FindFirstChild("ForceField") then
			print(player.Name .. " has died!")
			player.PlayerGui.Death.respawn.Visible = false
			local args = { [1] = "CIVILIAN" } game:GetService("ReplicatedStorage").TeamChanger:FireServer(unpack(args))
		end
	end
	_G.instrarespawn = true
end)

