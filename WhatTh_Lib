		a.Name = "Heal self"
		a.Parent = game.Players.LocalPlayer.Backpack
		a.RequiresHandle = false
		a.Activated:connect(function()
			local bc = -(game.Players.LocalPlayer.Character.Humanoid.MaxHealth - game.Players.LocalPlayer.Character.Humanoid.Health)
			dodmg(game.Players.LocalPlayer,bc)
		end)
		local a = Instance.new("Tool")
		a.ToolTip = "godmode"
		a.Name = "godmode (perm)"
		a.Parent = game.Players.LocalPlayer.Backpack
		a.RequiresHandle = false
		a.Activated:connect(function()
			dodmg(game.Players.LocalPlayer,-5000)
		end)
		local a = Instance.new("Tool")
		a.ToolTip = "armor"
		a.Name = "armor"
		a.Parent = game.Players.LocalPlayer.Backpack
		a.RequiresHandle = false
		a.Activated:connect(function()
			dodmg(game.Players.LocalPlayer,-200)
		end)
	else
		loopr_112.Text = "PREGENGUID NOT FOUND"
		wait(2)
		loopr_112.Text = "Heal Tools"
	end
end)

