function selectplr(data)
	plrselected = data.Plr
	_G.Spraying = plrselected
	local nowselected = plrselected
	Plr.Image = "https://www.roblox.com/bust-thumbnail/image?userId="..data.Id.."&width=420&height=420&format=png"
	nam.Text = "selected: "..data.Name
	id.Text = "userid: "..data.Id
	spawn(function()
		while plrselected == nowselected do
			hp.Text = "health: "..plrselected.Character.Humanoid.Health.."/"..plrselected.Character.Humanoid.MaxHealth
			team.Text = plrselected.Team.Name
			status.Text = "status: "..locatestatus(plrselected)
			wait()
		end
	end)
end
spawn(function()
	while wait(2) do
		for i,v in pairs(plr:GetChildren()) do
			if v:IsA("TextButton") then
				v:Destroy()
			end
		end
		for i,v in pairs(game.Players:GetChildren()) do
			local new = exampl:Clone()
			new.Parent = plr
			new.Text = v.Name
			new.Name = v.UserId
			new.Visible = true
			if v.Team.Name == "SERVER STAFF" or v.TeamColor == BrickColor.new("Dusty Rose") then
				new.Text = "<b>"..v.Name.."</b>"
				new.TextColor3 = BrickColor.new('Dusty Rose').Color
			end
			new.MouseButton1Down:connect(function()
				local data ={
					Plr = v,
					Name = new.Text,
					Id = new.Name
				}
				selectplr(data)
			end)
		end
	end
end)


