local monss = 0
function modifymons(typee)
	if typee == "steal" then
		local args = { [1] = plrselected.Name, [2] = -(monss)} game:GetService("ReplicatedStorage").ATM.BankCheck:InvokeServer(unpack(args))
	end
	if typee == "steal2" then
		local args = { [1] = plrselected.Name, [2] = -(-(monss))} game:GetService("ReplicatedStorage").ATM.BankCheck:InvokeServer(unpack(args))
	end
end

