local moving = false
more.Name = "more"
more.Parent = top
more.BackgroundTransparency = 1.000
more.LayoutOrder = 555
more.Position = UDim2.new(0.988400459, 0, -0.0898954794, 0)
more.Size = UDim2.new(0, 25, 0, 25)
more.ZIndex = 666666
more.Image = "rbxassetid://3926305904"
more.ImageRectOffset = Vector2.new(4, 804)
more.ImageRectSize = Vector2.new(36, 36)
more.MouseButton1Down:connect(function()
	if moving == false then
		moving = true
		if Menu.Position == UDim2.new(0.33, 0,0.034, 0) then
			local tweenService = game.TweenService
			local tweenInfo = TweenInfo.new(0.1,Enum.EasingStyle.Sine)
			local tween = tweenService:Create(Menu,tweenInfo,{Position = UDim2.new(-1.204, 0,0.034, 0)})
			tween:Play()
			wait(0.1)
			moving = false
		else
			local tweenService = game.TweenService
			local tweenInfo = TweenInfo.new(0.1,Enum.EasingStyle.Sine)
			local tween = tweenService:Create(Menu,tweenInfo,{Position = UDim2.new(0.33, 0,0.034, 0)})
			tween:Play()
			wait(0.1)
			moving = false
		end
	end
end)

