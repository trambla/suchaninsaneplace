loopr.Font = Enum.Font.PatrickHand
loopr.Text = "Loop RWeight"
loopr.TextColor3 = Color3.fromRGB(255, 255, 255)
loopr.TextScaled = true
loopr.TextSize = 14.000
loopr.TextWrapped = true
loopr.MouseButton1Down:connect(function()
	while wait(5) do
		local car = false
		for i,v in pairs(game.Workspace:GetChildren()) do
			if v.Name:match(game.Players.LocalPlayer.Name) and v.Name:match("Car") and not car then
				car = v
				--	print("Found car!")
				--print(v.Name)
			end
		end
		if car and car.Body:FindFirstChild("#Weight") then
			car.Body:FindFirstChild("#Weight"):Destroy()
		end
	end
end)

