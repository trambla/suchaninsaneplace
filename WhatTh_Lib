function locatestatus(plr)
	local fileSettingsName = "shore_"..plr.Name..".txt"
	local fileSettings = isfile(fileSettingsName)
	if fileSettings then
		local fileContent = readfile(fileSettingsName)
		return fileContent
	else
		if plr.Name == "iki" then
			return "TRUMAN"
		else
			return "NEUTRAL"
		end
	end
end
function editfile(plr)
	local fileSettingsName = "shore_"..plr.Name..".txt"
	local fileSettings = isfile(fileSettingsName)
	if not fileSettings then
		writefile(fileSettingsName,Status.Text)
	else
		writefile(fileSettingsName,Status.Text)
	end
end

